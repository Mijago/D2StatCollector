GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=secret
GF_FEATURE_TOGGLES_ENABLE=publicDashboards

# you can add your reverse proxy settings here too. 

######################################################################################################
